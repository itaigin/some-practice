{
    "name": "some-practice",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "serve": "webpack serve --env mode=development",
        "build:dev": "webpack --env mode=development",
        "build:prod": "webpack --env mode=production",
        "lint": "eslint src/**/*.{ts,js,jsx,tsx}",
        "lint:fix": "eslint src/**/*.{ts,js,jsx,tsx} --fix",
        "test": "jest",
        "lint-staged": "lint-staged",
        "prepare": "husky",
        "pretty:check": "prettier --check src/**/*.{ts,tsx,css,scss}",
        "pretty:write": "prettier --write src/**/*.{ts,tsx,css,scss}",
        "docker:build": "docker build -t some-practice .",
        "docker:run": "docker run -p 3000:3000 --name some-practice some-practice:latest",
        "docker:dev": "docker-compose -f docker-compose.dev.yml up",
        "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
        "typecheck": "tsc"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "dependencies": {
        "@faker-js/faker": "^10.2.0",

        "@babel/preset-env": "^7.28.6",
        "@babel/preset-react": "^7.28.5",
        "@babel/preset-typescript": "^7.28.5",
        "@eslint/js": "^9.39.1",
        "@jest/globals": "^30.2.0",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.6.2",
        "@types/faker": "^6.6.8",
        "@types/jest": "^30.0.0",
        "@types/node": "^25.0.9",
        "@types/react": "^19.2.8",
        "@types/react-dom": "^19.2.3",
